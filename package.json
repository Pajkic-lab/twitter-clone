  "scripts": {
    "// === Development ===": "",
    "dev:set": "node load-env.js dev && bash start-dev-postgres.sh && pnpm run app:set",
    "dev": "node load-env.js dev && bash start-dev-postgres.sh && concurrently \"npx nx serve server\" \"npx nx serve client\"",
    "// === Staging ===": "",
    "staging:set-local": "node load-env.js staging && bash start-staging-postgres.sh && pnpm run app:set",
    "staging-local": "node load-env.js staging && bash start-staging-postgres.sh && pnpm run test && npx nx e2e server-e2e",
    "staging:set-remote": "node load-env.js staging && pnpm run app:set",
    "staging-remote": "node load-env.js staging && pnpm run test && npx nx e2e server-e2e",
    "// === Production ===": "",
    "prod:set": "node load-env.js prod && pnpm run app:build",
    "prod": "pnpm run db:deploy && node dist/apps/server/main.js",
    "// === Build & Setup ===": "",
    "app:set": "pnpm i && pnpm run db:format && pnpm run db:generate && pnpm run db:migrate && pnpm run db:deploy",
    "app:build": "pnpm i && pnpm run db:generate && npx nx build client && npx nx build server",
    "// === Database (Prisma) ===": "",
    "db:deploy": "node prisma-cli.js migrate deploy",
    "db:format": "node prisma-cli.js format",
    "db:generate": "node prisma-cli.js generate",
    "db:gui": "node prisma-cli.js studio",
    "db:migrate": "node prisma-cli.js migrate dev",
    "db:reset": "node prisma-cli.js migrate reset --force && pnpm run db:migrate",
    "seed:user": "ts-node apps/server/src/modules/prisma/seeds/user.seed.ts",
    "// === Utilities ===": "",
    "env:set": "node setup-env.js",
    "storybook": "npx nx storybook client --port 6006",
    "check-dependencies": "depcheck",
    "app-structure": "node app-structure.js",
    "prepare": "husky",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,css,md,html,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,json,css,md,html,yml,yaml}\"",
    "// === Testing ===": "",
    "test": "npx nx run-many --target=test --all",
    "test-components": "npx nx run ui-components:test",
    "test-client": "npx nx run client:test",
    "// === Docs ===": "",
    "docs:dev": "pnpm i && nx serve docs",
    "docs:build": "nx build docs"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write"
    ],
    "*.{json,css,md,html,yml,yaml}": [
      "prettier --write"
    ]
  },
