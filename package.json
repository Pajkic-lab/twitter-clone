  "scripts": {
    "app:build": "pnpm i && pnpm run db:generate && npx nx build client && npx nx build server",
    "app:dev": "concurrently \"npx nx serve server\" \"npx nx serve client\"",
    "app:prod": "pnpm run db:deploy && npx nx serve server --prod",
    "app:set": "pnpm i && pnpm run db:format && pnpm run db:generate && pnpm run db:migrate && pnpm run db:deploy",
    "check-dependencies": "depcheck",
    "db:deploy": "prisma migrate deploy --schema=./libs/data-access/src/lib/prisma/schema.prisma",
    "db:format": "prisma format --schema=./libs/data-access/src/lib/prisma/schema.prisma",
    "db:generate": "prisma generate --schema=./libs/data-access/src/lib/prisma/schema.prisma",
    "db:gui": "prisma studio --schema=./libs/data-access/src/lib/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./libs/data-access/src/lib/prisma/schema.prisma",
    "db:reset": "prisma migrate reset --force && pnpm run db:migrate",
    "seed:user": "ts-node libs/data-access/src/lib/prisma/seeds/user.seed.ts",
    "sort-package-json": "sort-package-json"
  },
