  "scripts": {
    "dev": "node load-env.js dev && bash start-dev-postgres.sh && concurrently \"npx nx serve server\" \"npx nx serve client\"",
    "dev:set": "node load-env.js dev && bash start-dev-postgres.sh && pnpm run app:set",
    "staging-local": "node load-env.js staging && bash start-staging-postgres.sh && npx nx e2e server-e2e",
    "staging:set-local": "node load-env.js staging && bash start-staging-postgres.sh && pnpm run app:set",
    "staging-remote": "node load-env.js staging && npx nx e2e server-e2e",
    "staging:set-remote": "node load-env.js staging && pnpm run app:set",
    "prod": "pnpm run db:deploy && npx nx serve server --prod",
    "prod:set": "node load-env.js prod && pnpm run app:build",
    "db:deploy": "prisma migrate deploy --schema=./apps/server/src/modules/prisma/schema.prisma",
    "db:format": "prisma format --schema=./apps/server/src/modules/prisma/schema.prisma",
    "db:generate": "prisma generate --schema=./apps/server/src/modules/prisma/schema.prisma",
    "db:gui": "prisma studio --schema=./apps/server/src/modules/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./apps/server/src/modules/prisma/schema.prisma",
    "db:reset": "prisma migrate reset --force && pnpm run db:migrate",
    "seed:user": "ts-node apps/server/src/modules/prisma/seeds/user.seed.ts",
    "app:build": "pnpm i && pnpm run db:generate && npx nx build client && npx nx build server",
    "app:set": "pnpm i && pnpm run db:format && pnpm run db:generate && pnpm run db:migrate && pnpm run db:deploy",
    "check-dependencies": "depcheck",
    "sort-package-json": "sort-package-json",
    "app-structure": "node app-structure.js"
  },
